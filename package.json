{
  "name": "ember-orbit",
  "version": "0.17.0-beta.5",
  "description": "Ember.js data layer built with Orbit.js",
  "keywords": [
    "ember-addon",
    "orbit.js",
    "data",
    "synchronization"
  ],
  "repository": "https://github.com/orbitjs/ember-orbit",
  "license": "MIT",
  "contributors": [
    "Dan Gebhardt (http://www.cerebris.com)"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint . --ext .ts --ext .js",
    "lint:ts": "tsc --noEmit",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@glimmer/tracking": "^1.0.2",
    "@orbit/coordinator": "^0.17.0-beta.6",
    "@orbit/core": "^0.17.0-beta.4",
    "@orbit/data": "^0.17.0-beta.6",
    "@orbit/identity-map": "^0.17.0-beta.4",
    "@orbit/memory": "^0.17.0-beta.6",
    "@orbit/utils": "^0.17.0-beta.4",
    "ember-auto-import": "^1.8.0",
    "ember-cache-primitive-polyfill": "^1.0.1",
    "ember-cli-babel": "^7.23.0",
    "ember-cli-typescript": "^4.0.0",
    "ember-destroyable-polyfill": "^2.0.2",
    "ember-inflector": "^3.0.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@glimmer/component": "^1.0.2",
    "@types/ember": "^3.16.0",
    "@types/ember-qunit": "^3.4.11",
    "@types/ember-testing-helpers": "^0.0.4",
    "@types/qunit": "^2.9.4",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.23.0",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.23.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.14.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^10.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "qunit-dom": "^1.6.0",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "peerDependencies": {
    "ember-source": "~3.16.0"
  },
  "volta": {
    "node": "10.23.0",
    "yarn": "1.22.10"
  }
}
