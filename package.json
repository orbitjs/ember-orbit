{
  "name": "ember-orbit",
  "version": "0.17.0",
  "description": "Ember.js data layer built with Orbit.js",
  "keywords": [
    "ember-addon",
    "orbit.js",
    "data",
    "synchronization"
  ],
  "repository": "https://github.com/orbitjs/ember-orbit",
  "license": "MIT",
  "contributors": [
    "Dan Gebhardt (http://www.cerebris.com)"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:ts": "tsc --noEmit",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@orbit/coordinator": "^0.17.0",
    "@orbit/core": "^0.17.0",
    "@orbit/data": "^0.17.0",
    "@orbit/identity-map": "^0.17.0",
    "@orbit/memory": "^0.17.0",
    "@orbit/records": "^0.17.0",
    "@orbit/serializers": "^0.17.0",
    "@orbit/utils": "^0.17.0",
    "@orbit/validators": "^0.17.0",
    "ember-auto-import": "^2.2.4",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-typescript": "^4.2.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.49.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@types/ember": "^3.16.5",
    "@types/ember-qunit": "^3.4.15",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__test": "^3.16.1",
    "@types/ember__test-helpers": "^2.6.0",
    "@types/qunit": "^2.11.3",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.1.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.1.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "peerDependencies": {
    "ember-source": ">= 3.24.0"
  },
  "volta": {
    "node": "12.22.8",
    "yarn": "1.22.17"
  }
}
